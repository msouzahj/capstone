<h1><%=@meet.name %> Results</h1>

<%= link_to 'New Event', new_meet_event_path %><br><br>
<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Results</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.results %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

